{
  "name": "testish",
  "version": "1.1.0",
  "description": "Minimalistic E2E framework powered by Node.js and selenium-webdriver",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint \"+(config|globalHooks|lib|pages|runners|utils)/**/*.js\"",
    "precommit": "lint-staged"
  },
  "bin": "./bin/testish.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teologov/testish.git"
  },
  "keywords": [
    "e2e",
    "ui",
    "test",
    "tests",
    "framework",
    "mocha",
    "selenium",
    "chromedriver"
  ],
  "author": "Andrey Teologov <teologov.and@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teologov/testish/issues"
  },
  "homepage": "https://github.com/teologov/testish#readme",
  "dependencies": {
    "chai": "4.1.2",
    "chalk": "2.3.0",
    "chromedriver": "2.33.2",
    "co": "4.6.0",
    "co-mocha": "1.2.1",
    "del": "3.0.0",
    "minimist": "1.2.0",
    "mocha": "4.0.1",
    "selenium-webdriver": "3.6.0"
  },
  "devDependencies": {
    "eslint": "4.12.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-prettier": "2.3.1",
    "husky": "0.14.3",
    "lint-staged": "5.0.0",
    "prettier": "1.8.2"
  }
}
